{
  "name": "pizzalab",
  "version": "1.0.0",
  "description": "Demo NextJS app for a pizza restaurant.",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JuCollet/pizzaLab.git"
  },
  "scripts": {
    "coverage": "set NODE_ENV=test&& nyc yarn run test",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "mocha test/helper/browser.js --require babel-core/register --compilers jsx:babel-core/register \"test/**/*.test.{js,jsx}\" --require ignore-styles --no-deprecation",
    "test:watch": "mocha -w test/helper/browser.js --require babel-core/register --compilers jsx:babel-core/register \"test/**/*.test.{js,jsx}\" --require ignore-styles --no-deprecation"
  },
  "dependencies": {
    "next": "^5.0.0",
    "next-redux-wrapper": "^1.3.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "~9.0.0",
    "mocha": "^5.0.1",
    "nyc": "^11.4.1",
    "redux-test-utils": "^0.2.2",
    "sinon": "^4.3.0"
  }
}
